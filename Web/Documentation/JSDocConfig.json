{
    "plugins": [],
	
    "recurseDepth": 10,
	
    "source": {
		"include": ["../"],
		"exclude": [ "./", "../AxisEngine-full.js", "../AxisEngine-min.js", "../Projects" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
	
    "sourceType": "module",
	
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
	
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },

    "opts": {
        "template": "./templates/default",	// -t
        "encoding": "utf8",					// -e
        "destination": "./api/",			// -d
        "recurse": true,					// -r
        "tutorials": ""						// -u
    }
}
